<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/fondo">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="62dp"/>



    <Button
        android:text="Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_main"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="#dcc813" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:id="@+id/Ll_two"
        android:layout_height="60dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_sesenta"
            android:layout_weight="1"
            android:background="@drawable/sesentaa" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_setenta"
            android:layout_weight="1"
            android:background="@drawable/setentaa" />

        <Button
            android:layout_width="match_parent"
            android:id="@+id/bt_ochenta"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:background="@drawable/ochentaa" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_noventa"
            android:layout_weight="1"
            android:background="@drawable/noventaa" />
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_cien"
            android:layout_weight="1"
            android:background="@drawable/cienn" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:id="@+id/bt_target_one"
        android:layout_height="60dp"

        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp">

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_diez"
            android:layout_weight="1"
            android:background="@drawable/diezz" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_veinte"
            android:layout_weight="1"
            android:background="@drawable/veintee" />

        <Button
            android:layout_width="match_parent"
            android:id="@+id/bt_treinta"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:background="@drawable/treintaa" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_cuarenta"
            android:layout_weight="1"
            android:background="@drawable/cuarentaa" />
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/bt_cincuenta"
            android:layout_weight="1"
            android:background="@drawable/cincuentaa" />
    </LinearLayout>

</RelativeLayout>